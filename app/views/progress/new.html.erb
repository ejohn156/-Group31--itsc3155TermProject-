<h1>New Progress Update</h1>
<%= form_for :progress, url: progress_index_path do |f| %>
  <p>
    <%= f.label :date %><br>
    <%= f.date_select :date %>
  </p>
 
  <p>
    <%= f.label :weight %><br>
    <%= f.text_field :weight %>
    <%= "lb" %>
  </p>
 
  <p>
    <!-- <%= f.submit %> -->
    <%= link_to 'Submit', progress_index_path %>
  </p>
<% end %>